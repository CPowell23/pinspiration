<section class="board">
  <% if current_admin %>
    <%= link_to "Delete", admin_board_path(@board_presenter.board), method: :delete %>
  <% end %>
  <%= @board_presenter.board.name %>
  <%= @board_presenter.user.name %>
</section>

<% @board_presenter.pins.each do |pin| %>
  <%= pin.description %>
  <%= image_tag("#{pin.image_url}") %>
<% end %>
