<%= form_for @credentials do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :username %>
  <%= f.text_field :username %>

  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.submit "Save Settings" %>
<% end %>

<%= link_to "Cancel", registered_user_path(@credentials.username) %>
